#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    const char *jours[] = {
        "Lundi",
        "Mardi",
        "Mercredi",
        "Jeudi",
        "Vendredi",
        "Samedi",
        "Dimanche"
    };

    srand(time(NULL));

    int index = rand() % 7;

    printf("Random day: %s\n", jours[index]);

    return 0;
}
